{
  "name": "@foodsync/frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "clean": "rm -rf .nuxt .output node_modules",
    "capacitor:init": "npx cap init FoodSync com.foodsync.app --web-dir dist",
    "capacitor:add:ios": "npx cap add ios",
    "capacitor:sync": "npx cap sync",
    "ios:build": "pnpm generate && npx cap sync ios",
    "ios:open": "npx cap open ios"
  },
  "dependencies": {
    "@capacitor/app": "^6.0.1",
    "@capacitor/camera": "^6.0.2",
    "@capacitor/core": "^6.1.2",
    "@capacitor/haptics": "^6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/keyboard": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1",
    "@pinia/nuxt": "^0.5.5",
    "@vite-pwa/nuxt": "^1.1.0",
    "nuxt": "^3.14.0",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@nuxt/eslint": "^0.6.1",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@types/node": "^22.9.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.14.2"
}
